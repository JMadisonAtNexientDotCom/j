    <servlet>
        <servlet-name>MyServletsName</servlet-name>
        <servlet-class>com.myPak.hey.HelloServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>MyServletsName</servlet-name>
        <url-pattern>/hi</url-pattern>
    </servlet-mapping>
		
    
</web-app>

